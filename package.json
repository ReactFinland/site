{
  "name": "react-finland-site",
  "version": "0.0.0",
  "description": "React Finland site",
  "main": "index.js",
  "private": true,
  "scripts": {
    "start": "node ./antwar.bootstrap.js develop",
    "build":
      "node ./antwar.bootstrap.js build && npm run sitemap && npm run copy:all",
    "build:netlify": "npm run build",
    "deploy": "gh-pages -d build",
    "lint": "eslint . --ext js --ext jsx --ignore-path .gitignore --cache",
    "test": "npm run lint",
    "sitemap":
      "cd build && sitemap-static --prefix=https://react-finland.fi/ > sitemap.xml",
    "copy:all": "npm run copy:extra",
    "copy:extra": "cp -rfv assets/extra/* build",
    "precommit": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:ReactFinland/site.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ReactFinland/site/issues"
  },
  "homepage": "https://github.com/ReactFinland/site#readme",
  "dependencies": {
    "@react-finland/content-2018": "^11.2.7",
    "antwar": "^0.21.1",
    "antwar-interactive": "^0.21.0",
    "classnames": "^2.2.5",
    "moment": "^2.20.1",
    "normalize.css": "^7.0.0",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-country-flag": "^0.1.12",
    "react-dom": "^16.2.0",
    "react-images": "^0.5.14",
    "react-markdown": "^3.1.4",
    "react-photo-gallery": "^6.0.22",
    "react-router-dom": "^4.2.2"
  },
  "devDependencies": {
    "autoprefixer": "^7.2.5",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-register": "^6.26.0",
    "clean-webpack-plugin": "^0.1.17",
    "css-loader": "^0.28.9",
    "eslint": "^4.16.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.5.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "gh-pages": "^1.1.0",
    "husky": "^0.14.3",
    "lint-staged": "^6.0.1",
    "node-sass": "^4.7.2",
    "postcss-loader": "^2.0.10",
    "preact": "^8.2.7",
    "preact-compat": "^3.18.0",
    "prettier": "^1.10.2",
    "raw-loader": "^0.5.1",
    "react-router": "^4.2.0",
    "sass-loader": "^6.0.6",
    "sitemap-static": "^0.4.2",
    "style-loader": "^0.19.1",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1",
    "webpack-merge": "^4.1.1"
  },
  "lint-staged": {
    "*.{js,jsx,css,less,json}": ["prettier --write", "git add"]
  }
}
